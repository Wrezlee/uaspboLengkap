   <property name="styleSheet">
    <string notr="true">/* ========================================================== */
/* STYLING KHUSUS UNTUK QTabWidget (Tambah, Edit, Hapus) */
/* ========================================================== */

/* --- QTabWidget &amp; QTabBar Styling --- */
QTabWidget::pane {
    /* Area konten tab (pane) */
    border: 2px solid #90CAF9; /* Border biru muda yang halus */
    border-radius: 10px;
    padding: 1px;
    background-color:  #E3F2FD; /* Latar belakang konten tab: Biru Muda (Tepat seperti yang Anda minta) */
}

QTabWidget::tab-bar {
    left: 5px; /* Menggeser sedikit tab header ke kanan */
}

QTabBar::tab {
    /* Gaya untuk tab yang tidak terpilih */
    background: #BBDEFB; /* Biru sedang */
    border: 1px solid #90CAF9; 
    border-bottom-color: #90CAF9; /* Border bawah sesuai warna border pane */
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    padding: 8px 15px;
    min-width: 80px;
    font-weight: bold;
    color: #1976D2; /* Teks biru gelap */
}

QTabBar::tab:selected, QTabBar::tab:hover {
    /* Gaya untuk tab yang terpilih atau di-hover */
    background: white; /* Latar belakang putih */
    border-color: #90CAF9;
    border-bottom-color: white; /* Agar terlihat menyambung dengan content pane */
}

/* --- QGroupBox Styling di dalam Tab (Area Form) --- */
QGroupBox { 
    background-color: white; /* Latar belakang kotak input putih */
    border: 2px solid #BBDEFB; /* Border biru muda */
    border-radius: 10px; 
    margin-top: 10px; 
    padding-top: 20px; 
} 
QGroupBox::title { 
    subcontrol-origin: margin; 
    subcontrol-position: top center;
    padding: 0 10px; 
    color: rgba(0, 0, 0, 180); 
    font: 600 14pt &quot;Inter&quot;;
}

/* --- Input Fields (QLineEdit, QSpinBox) Styling di dalam Tab --- */
QLineEdit { 
    border: 1px solid #BBDEFB; /* Border biru muda */
    border-radius: 6px; 
    padding: 8px; 
    color: rgba(0, 0, 0, 180); 
}
QLineEdit:focus { 
    border: 2px solid #9C27B0; /* Fokus ungu yang lebih menonjol */
}
QSpinBox { 
    border: 1px solid #BBDEFB;
    border-radius: 6px; 
    padding: 8px; 
    color: rgba(0, 0, 0, 180); 
}

/* --- QPushButton Styling di dalam Tab --- */
/* Umumkan gaya dasar tombol di dalam tab */
QTabWidget QPushButton {
    border: none;
    border-radius: 8px;
    padding: 10px; 
    font: 600 11pt &quot;Inter&quot;;
    color: white;
    min-height: 30px;
}

/* Button Simpan/Update (Hijau) */
QPushButton#pushButton_simpan, QPushButton#pushButton_update { 
    background-color: #4CAF50; /* Hijau */
} 
QPushButton#pushButton_simpan:hover, QPushButton#pushButton_update:hover { 
    background-color: #388E3C; /* Hijau lebih gelap */
}

/* Button Hapus/Kembali/Batal (Merah) */
QPushButton#pushButton_hapus, 
QPushButton#pushButton_kembali, 
QPushButton#pushButton_cancel { 
    background-color: #F44336; /* Merah */
} 
QPushButton#pushButton_hapus:hover, 
QPushButton#pushButton_kembali:hover, 
QPushButton#pushButton_cancel:hover { 
    background-color: #D32F2F; /* Merah lebih gelap */
}

/* Button Cari (Biru muda) */
QPushButton#pushButton_cari_pakaian { 
    background-color: #BBDEFB; 
    color: black; 
    font: 500 10pt &quot;Inter&quot;; 
    min-height: 10px;
    padding: 0px;
} 
QPushButton#pushButton_cari_pakaian:hover { 
    background-color: #90CAF9; 
}</string>
